		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.17",
		"@types/eslint": "^9.6.0",
		"@types/node": "^22",
		"@types/prismjs": "^1.26.5",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.21.16",
		"clsx": "^2.1.1",
		"dompurify": "^3.2.1",
